	"license": "MIT",
	"keywords": ["PHP", "stream", "split", "file", "string", "character"],
	"authors": [
		{
			"name": "Jack Clayton and Contributors",
			"homepage": "https://github.com/jstewmc/php-helpers/contributors"
		}
	],
	"require": {
